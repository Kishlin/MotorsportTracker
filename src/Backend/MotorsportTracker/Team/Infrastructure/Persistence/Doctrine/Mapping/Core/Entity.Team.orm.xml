<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="Kishlin\Backend\MotorsportTracker\Team\Domain\Entity\Team" table="teams">
        <id name="id" type="uuid_value_object" column="id" length="36" />

        <field name="country" type="uuid_value_object" column="country" length="36" />

        <field name="slug" type="string_value_object" column="slug" unique="true" />

        <field name="name" type="string_value_object" column="name" />

        <field name="code" type="string_value_object" column="code" />
    </entity>

</doctrine-mapping>
