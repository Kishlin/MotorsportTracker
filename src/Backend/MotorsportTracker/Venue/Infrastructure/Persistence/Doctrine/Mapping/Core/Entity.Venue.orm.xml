<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="Kishlin\Backend\MotorsportTracker\Venue\Domain\Entity\Venue" table="venues">
        <id name="id" type="venue_id" column="id" length="36" />

        <field name="name" type="venue_name" column="name" unique="true" />

        <field name="countryId" type="venue_country_id" column="country" length="36" />
    </entity>

</doctrine-mapping>
