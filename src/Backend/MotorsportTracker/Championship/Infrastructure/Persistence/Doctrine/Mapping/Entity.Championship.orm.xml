<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="Kishlin\Backend\MotorsportTracker\Championship\Domain\Entity\Championship" table="championships">
        <id name="id" type="championship_id" column="id" length="36" />

        <field name="name" type="championship_name" column="name" unique="true" />

        <field name="slug" type="championship_slug" column="slug" unique="true" length="36" />
    </entity>

</doctrine-mapping>
