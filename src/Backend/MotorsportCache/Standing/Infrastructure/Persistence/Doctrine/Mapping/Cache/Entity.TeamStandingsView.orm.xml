<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="Kishlin\Backend\MotorsportCache\Standing\Domain\Entity\TeamStandingsView" table="team_standings_views">
        <id name="id" type="standings_view_id" column="id" length="36" />

        <field name="championshipSlug" type="standings_view_championship_slug" column="championship_slug"/>

        <field name="year" type="standings_view_year" column="year" min="0" />

        <field name="events" type="standings_view_events" column="events"/>

        <field name="standings" type="standings_view_standings" column="standings"/>

        <unique-constraints>
            <unique-constraint columns="championship_slug,year" name="team_standings_views_championship_year_idx" />
        </unique-constraints>
    </entity>

</doctrine-mapping>
